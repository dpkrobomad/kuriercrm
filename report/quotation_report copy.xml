<odoo>
    <data>
        <report
            id="sale_custome_report_quotation_report"
            model="sale.order"
            string="Quotation Report"
            report_type="qweb-html"
            name="deepu_sale.report_sale_custome_quotation_report"
            file="deepu_sale.report_sale_custome_quotation_report"
            print_report_name="'Quotation Report-%s' %(object.name)"
            paperformat="paper_format_sale_custome"
        />
        
        <template id="report_sale_custome_quotation_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="deepu_sale.sale_custome_external_layout"></t>
                    <t t-set="company" t-value="doc.company_id"/>
                    <div class="row">
                            <h1 style="text-align: left;"> Quotation Airfreight Import</h1>
                            <img  t-if="doc.company_id.logo" t-att-src="image_data_uri(doc.company_id.logo)" style="max-height: 60px;" alt="Logo"/>
                            
                    </div>
                    
                </t>
            </t>
        </template>
    </data>
</odoo>