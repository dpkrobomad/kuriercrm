<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Bill of Lading uses tracking number (sale.tracking sequence) - no separate seaway.bill sequence -->

    <!-- Freight options: Freight Prepaid, Freight Collect (countries synced via init) -->
    <record id="freight_option_prepaid" model="seaway.freight.option">
        <field name="name">Freight Prepaid</field>
        <field name="sequence">1</field>
    </record>
    <record id="freight_option_collect" model="seaway.freight.option">
        <field name="name">Freight Collect</field>
        <field name="sequence">2</field>
    </record>

    <!-- Countries are synced by data/seaway_sync_countries.xml (runs on every upgrade) -->
</odoo>
